{
  "name": "eruda-timing",
  "version": "1.1.1",
  "main": "eruda-timing.js",
  "description": "Eruda plugin for performance and resource timing",
  "scripts": {
    "dev": "webpack-dev-server --host 0.0.0.0",
    "build": "webpack && webpack -p",
    "format": "prettier src/index.js src/style.scss *.js --write",
    "ci": "npm run format && npm run lint && npm run build",
    "lint": "eslint src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/eruda-timing.git"
  },
  "keywords": [
    "eruda",
    "plugin",
    "timing"
  ],
  "eustia": {
    "files": "./src/**/*.js",
    "output": "./src/util.js",
    "format": "commonjs"
  },
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eruda-timing/issues"
  },
  "homepage": "https://github.com/liriliri/eruda-timing#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "eruda": "^1.3.2",
    "eslint": "^4.12.1",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.14",
    "postcss-class-prefix": "^0.3.0",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.13.7",
    "sass-loader": "^6.0.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
