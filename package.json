{
  "name": "eruda-timing",
  "version": "2.0.0",
  "main": "eruda-timing.js",
  "description": "Eruda plugin for performance and resource timing",
  "scripts": {
    "dev": "webpack-dev-server --host 0.0.0.0",
    "build": "webpack -p",
    "format": "prettier src/index.js src/style.scss *.js index.html --write",
    "ci": "npm run lint && npm run build",
    "lint": "eslint src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/eruda-timing.git"
  },
  "keywords": [
    "eruda",
    "plugin",
    "timing"
  ],
  "eustia": {
    "files": "./src/**/*.js",
    "output": "./src/util.js",
    "format": "commonjs"
  },
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eruda-timing/issues"
  },
  "homepage": "https://github.com/liriliri/eruda-timing#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime": "^7.7.2",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eruda": "^2.0.0",
    "eslint": "^6.6.0",
    "handlebars": "^4.5.1",
    "handlebars-loader": "^1.7.1",
    "node-sass": "^4.13.0",
    "postcss": "^7.0.21",
    "postcss-class-prefix": "^0.3.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
